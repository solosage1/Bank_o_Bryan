{
  "name": "bank-o-bryan",
  "version": "1.0.0",
  "description": "Family banking application for kids aged 10-14",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "ci:guard-check": "grep -R -nE \"status\\s*===\\s*'loading'\\s*\\|\\|\\s*status\\s*===\\s*'ready'\" src || true",
    "ci:redirect-check": "grep -R -nE \"useRequireAuth\\(|useRedirectOnReady\\(\" src || true",
    "ci:report": "echo 'Guard checks:' && pnpm -s ci:guard-check && echo '\nRedirect usage:' && pnpm -s ci:redirect-check",
    "test": "vitest run tests/unit",
    "test:watch": "vitest",
    "e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
